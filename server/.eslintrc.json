{
  "root": true,
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }]
  },
  "ignorePatterns": [
    "dist/",
    "node_modules/",
    "**/gaiasnapshot/**",
    "**/venv/**",
    "**/__pycache__/**",
    "*.pyc",
    "node_modules/",
    "dist/",
    ".next/"
  ],
  "env": {
    "node": true,
    "es6": true
  }
}
